<h1>Your Bookings</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>Number of Tickets</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.event.name %></td>
        <td><%= booking.number_of_tickets %></td>
        <td><%= booking.created_at.strftime('%B %d, %Y') %></td>
      </tr>
    <% end %>
  </tbody>
</table>